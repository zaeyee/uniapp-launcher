{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "outDir": "out",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "strict": true,
    "types": ["@types/node", "@types/debug", "@types/fs-extra"]
  }
}
