{
  "name": "uniapp-launcher",
  "displayName": "uniapp-launcher",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:uniapp-launcher.launch"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "category": "uniapp-launcher",
        "command": "uniapp-launcher.launch",
        "title": "launch",
        "icon": "$(play)"
      }
    ],
    "keybindings": [
      {
        "command": "uniapp-launcher.launch",
        "key": "ctrl+f1",
        "mac": "cmd+f1",
        "when": "resourceFilename == manifest.json"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "uniapp-launcher.launch",
          "group": "navigation",
          "when": "resourceFilename == manifest.json"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@vscode/test-electron": "^2.2.2",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "uniapp-launcher": "workspace:*"
  }
}
